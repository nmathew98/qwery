{
	"name": "@b.s/qwery",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/nmathew98/qwery"
	},
	"exports": {
		"import": "./dist/qwery.js",
		"require": "./dist/qwery.umd.cjs",
		"types": "./dist/index.d.ts"
	},
	"description": "Simple composable async state management utilities for React",
	"license": "MIT",
	"keywords": [
		"crdt",
		"react",
		"query",
		"asynchronous",
		"state",
		"management",
		"utilities",
		"composable",
		"database",
		"universal",
		"distributed",
		"transaction",
		"saga"
	],
	"files": [
		"dist",
		"LICENSE",
		"package.json",
		"pnpm-lock.yaml",
		"README.md"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"format": "prettier .  --write",
		"preview": "vite preview",
		"test": "vitest run --passWithNoTests",
		"test.watch": "vitest --passWithNoTests --watch"
	},
	"dependencies": {
		"@b.s/incremental": "^2.1.1",
		"@b.s/txn": "^1.0.3"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@testing-library/react": "^14.2.1",
		"@types/node": "^20.11.19",
		"@types/react": "^18.2.56",
		"@types/react-dom": "^18.2.19",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"jsdom": "^24.0.0",
		"prettier": "^3.2.5",
		"react-dom": "^18.2.0",
		"typescript": "^5.2.2",
		"vite": "^5.1.4",
		"vite-plugin-dts": "^3.7.3",
		"vitest": "^1.3.1"
	}
}
