{
	"name": "@b.s/qwery-example-react",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"clean": "rm -f *.tgz && rm -rf dist",
		"format": "prettier .  --write",
		"test": "vitest run --passWithNoTests",
		"test.watch": "vitest --passWithNoTests --watch",
		"pack": "pnpm pack"
	},
	"dependencies": {
		"@b.s/react-qwery": "workspace:^",
		"@b.s/s.o.o.n": "^1.0.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@b.s/qwery-example-api": "../example-api",
		"@tailwindcss/vite": "4.0.0-alpha.7",
		"@types/react": "^18.2.56",
		"@types/react-dom": "^18.2.19",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"prettier": "^3.2.5",
		"tailwindcss": "4.0.0-alpha.7",
		"typescript": "^5.2.2",
		"vite": "^5.1.4"
	}
}
