{
	"name": "@b.s/qwery-shared",
	"version": "1.5.5",
	"private": true,
	"type": "module",
	"devDependencies": {
		"@b.s/incremental": "^2.1.7",
		"acorn": "^8.11.3",
		"esbuild": "^0.20.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"prettier": "^3.2.5",
		"tsup": "^8.0.2",
		"typescript": "~5.4.2"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
