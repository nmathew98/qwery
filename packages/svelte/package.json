{
	"name": "@b.s/svelte-qwery",
	"version": "1.5.0",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/nmathew98/qwery"
	},
	"main": "./dist/svelte-qwery.umd.cjs",
	"module": "./dist/svelte-qwery.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": "./dist/svelte-qwery.js",
		"require": "./dist/svelte-qwery.umd.cjs",
		"types": "./dist/index.d.ts"
	},
	"description": "Simple composable async state management utilities for Svelte",
	"license": "MIT",
	"keywords": [
		"crdt",
		"svelte",
		"query",
		"asynchronous",
		"state",
		"management",
		"utilities",
		"composable",
		"database",
		"universal",
		"distributed",
		"transaction",
		"saga"
	],
	"files": [
		"dist",
		"LICENSE",
		"package.json",
		"pnpm-lock.yaml",
		"README.md",
		"tsconfig.json",
		"tsconfig.node.json"
	],
	"scripts": {
		"dev": "vite",
		"build": "svelte-check --tsconfig ./tsconfig.json && vite build",
		"clean": "rm -f *.tgz && rm -rf dist",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"format": "prettier .  --write",
		"preview": "vite preview",
		"test": "vitest run --passWithNoTests",
		"test.watch": "vitest --passWithNoTests --watch",
		"pack": "pnpm pack"
	},
	"dependencies": {
		"@b.s/incremental": "^2.1.4",
		"@b.s/txn": "^1.0.4"
	},
	"devDependencies": {
		"@b.s/qwery-shared": "link:../shared",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tsconfig/svelte": "^5.0.2",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"jsdom": "^24.0.0",
		"prettier": "^3.2.5",
		"svelte": "^4.2.11",
		"svelte-check": "^3.6.4",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^5.1.4",
		"vite-plugin-dts": "^3.7.3",
		"vitest": "^1.3.1"
	}
}
